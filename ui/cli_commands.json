      {"command":"%W",                                                    "function":"exec_auto_answer"}]},
    {"command":"auto_connect",    "help":"Auto Connect",                                                                              "subcommands":[
      {"command":"%W",                                                    "function":"exec_auto_connect"}]},
    {"command":"mac",             "help":"Device MAC",                                                                                "subcommands":[
      {"command":"%W",                                                    "function":"exec_set_mac"}]},
    {"command":"model",           "help":"Phone model",                                                                               "subcommands":[
      {"command":"%W",                                                    "function":"exec_set_model"}]},
    {"command":"server",          "help":"CUCM address/host",                                                                         "subcommands":[
      {"command":"%W",                                                    "function":"exec_set_server"}]},
    {"command":"enable_cdp",      "help":"Enable CDP TX/RX",                                                                          "subcommands":[
      {"command":"%W",                                                    "function":"exec_set_cdp"}]},
    {"command":"enable_lldp",     "help":"Enable LLDP TX/RX",                                                                         "subcommands":[
      {"command":"%W",                                                    "function":"exec_set_lldp"}]},
    {"command":"enable_lsp",      "help":"Enable Meraki LSP Polling",                                                                 "subcommands":[
      {"command":"%W",                                                    "function":"exec_set_lsp"}]}
  ]},
  {"command": "phone",            "help": "Softphone commands",                                                                       "subcommands": [
