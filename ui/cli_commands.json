      {"command": "%W",           "help": "Extension or phone number",    "function": "exec_call_number"}
    ]},
    {"command": "send",           "help": "Send skinny message to call manager",                                                      "subcommands": [
      {"command": "softkeyevent", "help": "Send softkeyevent message",                                                                "subcommands": [
        {"command": "%W",         "help": "Softkey name",                                                         "optional": false,  "subcommands": [
          {"command": "%W",       "help": "Line number",                                                          "optional": true,   "subcommands": [
            {"command": "%W",     "help": "Call reference (call_id)",     "function": "exec_send_softkeyevent",   "optional": true}
          ]}
        ]}
      ]},
      {"command": "digit",        "help": "Send phone digit",                                                                         "subcommands": [
        {"command": "%W",         "help": "Digit (0-9,*,#)",              "function": "exec_send_digit",          "optional": false}
      ]}
    ]}
  ]},
  {"command": "show",             "help": "Show information for a given object",                                                      "subcommands": [
    {"command":"config",          "help": "Show configuration",           "function":"exec_show_config"},
    {"command": "calls",          "help": "Show current calls",           "function": "exec_show_calls"},
    {"command": "state",          "help": "Show system state",            "function": "exec_show_state_obj",                          "subcommands": [
      {"command": "%W",           "help": "Show specific system state",   "function": "exec_show_state_obj",      "optional": false}
    ]},
    {"command": "topology",       "help": "Show network topology",        "function": "exec_command_topology",                        "subcommands": [
      {"command": "detail",       "help": "Show detailed topology data",  "function": "exec_command_topology",    "optional": true}
    ]}
  ]},
  {"command": "exit", "help": "Exit command mode"}
